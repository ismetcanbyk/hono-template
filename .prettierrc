{
	"semi": true,
	"trailingComma": "all",
	"singleQuote": false,
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["^@/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
